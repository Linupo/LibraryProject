@model LibraryProject.Models.WorkerModel

@{
    ViewBag.Title = Model.Tier;
}

<div class="text-center" style="margin-top: 26px; margin-bottom: 26px;">
    <h2>@Model.Template (@Model.Tier)</h2>
    <h5>ID: @Model.Id</h5>
</div>

<div class="row">
    <div class="col-xs-12 col-sm-4">
        <img src="data:image/jpeg;base64," class="center-block book-img" />
    </div>
    <p class="col-xs-12 col-sm-8">@Model.Description</p>
</div>

